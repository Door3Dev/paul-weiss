{
    "sass.enabled": false,
    "path": {
        "scss/*.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "includePaths": [],
                "sourceComments": false,
                "outputStyle": "nested"
            },
            "linting.collapsed": true,
            "debug.showErrorsInStatusBar": true
        }
    }
}